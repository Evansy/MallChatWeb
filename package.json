    "element-plus": "^2.3.4",
    "gsap": "^3.11.5",
    "lodash": "^4.17.21",
    "pinia": "^2.0.35",
    "qrcode.vue": "^3.4.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
